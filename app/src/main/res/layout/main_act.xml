<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.99"
            android:id="@+id/glH99"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.93"
            android:id="@+id/glH93"/>




        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/glH92"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.92" />

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.02"
            android:id="@+id/glV2"/>
        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.98"
            android:id="@+id/glV98"/>

        <FrameLayout
            android:background="@color/white"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toTopOf="@id/glH92"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/fragment_container_main"/>


        <TextView
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/glH92"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="#3E005DAF">


        </TextView>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.212"
            android:id="@+id/glV21.2"/>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.404"
            android:id="@+id/glV40.4"/>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.596"
            android:id="@+id/glV59.6"/>

        <androidx.constraintlayout.widget.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.788"
            android:id="@+id/glV78.8"/>

        <LinearLayout
            android:id="@+id/btn_home_off"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV21.2"
            app:layout_constraintLeft_toRightOf="@id/glV2"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:src="@drawable/ic_home_off"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/off"
                android:textStyle="bold"
                android:text="@string/btn_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_home_on"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV21.2"
            app:layout_constraintLeft_toRightOf="@id/glV2"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:src="@drawable/ic_home_on"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/btn_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_category_off"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV40.4"
            app:layout_constraintLeft_toRightOf="@id/glV21.2"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="4dp"
                android:layout_weight="1"
                android:src="@drawable/ic_category_off"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/off"
                android:textStyle="bold"
                android:text="@string/btn_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_category_on"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV40.4"
            app:layout_constraintLeft_toRightOf="@id/glV21.2"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="4dp"
                android:layout_weight="1"
                android:src="@drawable/ic_category_on"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/btn_category"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_cart"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV59.6"
            app:layout_constraintLeft_toRightOf="@id/glV40.4"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="1dp"
                android:layout_weight="1"
                android:src="@drawable/ic_cart"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/off"
                android:textStyle="bold"
                android:text="@string/btn_cart"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_chat"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV78.8"
            app:layout_constraintLeft_toRightOf="@id/glV59.6"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:src="@drawable/ic_chat"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/off"
                android:textStyle="bold"
                android:text="@string/btn_chat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_user_off"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV98"
            app:layout_constraintLeft_toRightOf="@id/glV78.8"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:src="@drawable/ic_user_off"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/off"
                android:textStyle="bold"
                android:text="@string/btn_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_user_on"
            android:gravity="center_horizontal"
            app:layout_constraintRight_toRightOf="@id/glV98"
            app:layout_constraintLeft_toRightOf="@id/glV78.8"
            app:layout_constraintBottom_toTopOf="@id/glH99"
            app:layout_constraintTop_toBottomOf="@id/glH93"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical">

            <ImageView
                android:layout_marginTop="2dp"
                android:layout_weight="1"
                android:src="@drawable/ic_user_on"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"/>

            <TextView
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="@string/btn_user"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
